{
	"name": "pharmacy-db",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --mode dev",
		"build": "vite build",
		"preview": "pnpm run build && wrangler dev .svelte-kit/cloudflare/_worker.js --assets .svelte-kit/cloudflare/",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:migrate:local": "drizzle-kit generate && wrangler d1 migrations apply DB",
		"deploy": "pnpm run build && wrangler deploy",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.13.13",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.38.0",
		"@iconify-json/material-symbols": "^1.2.42",
		"@iconify-json/mdi": "^1.2.3",
		"@libsql/client": "^0.14.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.47.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.15",
		"@types/node": "^22.18.12",
		"drizzle-kit": "^0.30.6",
		"drizzle-orm": "^0.40.1",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.5",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.41.1",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.15",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2",
		"vite": "^7.1.11",
		"wrangler": "^4.43.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"overrides": {
			"esbuild@<=0.24.2": ">=0.25.0",
			"cookie@<0.7.0": ">=0.7.0"
		}
	},
	"dependencies": {
		"@iconify/types": "^2.0.0",
		"better-auth": "^1.3.28",
		"fuse.js": "^7.1.0",
		"m3-svelte": "^5.12.0"
	}
}
